{
  "name": "tinject",
  "version": "1.0.0",
  "description": "Shim for commonjs require when running tests on node",
  "main": "dist/index.js",
  "scripts": {
    "transpile": "babel -d dist/ src/",
    "babel-watch": "babel -w src/ -d dist/",
    "test": "./node_modules/.bin/mocha --reporter spec specs",
    "test-watch": "./node_modules/.bin/mocha --reporter spec specs --watch"
  },
  "author": "Simon Puckey",
  "license": "ISC",
  "dependencies": {
    "mocha": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "expect": "^1.20.2",
    "rewire": "^2.5.2"
  }
}
